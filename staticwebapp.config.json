{
  "mimeTypes": {
    ".xml": "text/xml",
    ".html": "text/html",
    ".js": "application/javascript",
    ".png": "image/png"
  },
  "routes": [
    {
      "route": "/manifest.xml",
      "serve": "/manifest.xml",
      "statusCode": 200
    },
    {
      "route": "/taskpane.html",
      "serve": "/taskpane.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ]
}
